{
  "name": "flappybird",
  "description": "Mimicking the old retro game FlappyBird but as a web app",
  "version": "0.5.5",
  "main": "vite.config.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "start": "npm run dev",
    "prettier-format": "prettier --config .prettierrc \"./src\" \"./types\" --write",
    "prettier-format:md": "prettier --config .prettierrc \"./*.js\" --write",
    "lint": "eslint ./src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxmked/Flappybird.git"
  },
  "keywords": [
    "canvas",
    "2d-games",
    "typescript",
    "opensource",
    "retro"
  ],
  "author": "Jovan De Guia",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jxmked/Flappybird/issues"
  },
  "homepage": "https://jxmked.github.io/Flappybird/",
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.3.7",
    "@types/raf": "^3.4.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@types/node": "^20.14.2",
    "@types/three": "^0.170.0",
    "eslint": "^8.57.1",
    "prettier": "^2.8.8",
    "raf": "^3.4.1",
    "sass": "^1.83.4",
    "typescript": "^4.9.5",
    "vite": "^5.4.3",
    "vitest": "^1.6.0",
    "jsdom": "^24.0.0",
    "@vitest/coverage-v8": "^1.6.0"
  },
  "dependencies": {
    "@solid-primitives/raf": "^2.3.0",
    "gh-pages": "^6.3.0",
    "three": "^0.170.0"
  }
}
